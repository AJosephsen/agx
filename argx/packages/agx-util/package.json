{
    "name": "agx-util",
    "description": "Utility package for converting AGX AST into TypeScript graph structures for analysis",
    "version": "0.0.1",
    "type": "module",
    "engines": {
        "node": ">=20.10.0",
        "npm": ">=10.2.3"
    },
    "files": [
        "out",
        "src"
    ],
    "main": "./out/index.js",
    "module": "./out/index.js",
    "exports": {
        ".": {
            "types": "./out/index.d.ts",
            "default": "./out/index.js"
        }
    },
    "typesVersions": {
        "*": {
            ".": [
                "out/index"
            ]
        }
    },
    "scripts": {
        "clean": "shx rm -fr *.tsbuildinfo out",
        "build": "echo 'No build step'",
        "build:clean": "npm run clean && npm run build",
        "test": "vitest run"
    },
    "dependencies": {
        "achitecture-graph-language": "0.0.1",
        "langium": "~4.2.0"
    },
    "devDependencies": {
        "vitest": "~3.1.3"
    }
}
